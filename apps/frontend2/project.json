{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/frontend2/src",
  "targets": {
    "build": {
      "executor": "@nxext/sveltekit:sveltekit",
      "options": {
        "command": "build"
      }
    },
    "serve": {
      "executor": "@nxext/sveltekit:sveltekit",
      "options": {
        "command": "dev --port 8080"
      }
    },
    "add": {
      "executor": "@nxext/sveltekit:add"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/frontend2/**/*.{ts,svelte,spec.ts}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/frontend2"],
      "options": {
        "jestConfig": "apps/frontend2/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
